{
  "proxy": "http://localhost:3000",
  "port": 3001,
  "ui": {
    "port": 3002
  },
  "files": [
    "src/frontend/static/**/*"
  ],
  "watchOptions": {
    "ignoreInitial": true,
    "ignored": [
      "node_modules",
      "*.test.js",
      "**/__tests__/**"
    ]
  },
  "open": true,
  "notify": false,
  "logLevel": "info",
  "reloadDelay": 300,
  "injectChanges": true,
  "codeSync": true
}