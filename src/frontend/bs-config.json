{
   "proxy": "http://localhost:3000",
   "port": 3001,
   "ui": {
      "port": 3002
   },
   "files": ["static/**/*"],
   "watchOptions": {
      "ignoreInitial": true,
      "ignored": ["node_modules/**", "**/*.test.js", "**/__tests__/**", "src/backend/**"]
   },
   "open": false,
   "notify": false,
   "logLevel": "info",
   "reloadDelay": 300,
   "injectChanges": true
}
